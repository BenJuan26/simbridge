{
	"name": "a32nx-local-api",
	"version": "0.0.1",
	"description": "",
	"author": "",
	"private": true,
	"license": "GNu",
	"bin": {
		"fbw-local-api": "./bin/index.js"
	},
	"scripts": {
		"prebuild": "rimraf dist",
		"build": "nest build",
		"build:exec": "npm run build && pkg --public-packages '*' -t node16-win-x64 --output dist/local-server.exe dist/main.js",
		"build:exec:mac": "npm run build && pkg --public-packages '*' -t node16-macos-x64 --output dist/local-server dist/main.js ",
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "webpack --config webpack.config.js --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/main",
		"lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config ./test/jest-e2e.json"
	},
	"dependencies": {
		"@nestjs/common": "^8.0.0",
		"@nestjs/core": "^8.0.0",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/swagger": "^5.1.5",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.2",
		"nest-winston": "^1.6.2",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"swagger-ui-express": "^4.3.0",
		"winston": "^3.3.3",
		"winston-daily-rotate-file": "^4.5.5",
		"xml2js": "^0.4.23"
	},
	"devDependencies": {
		"@flybywiresim/eslint-config": "^0.2.3",
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "27.0.2",
		"@types/node": "^16.0.0",
		"@types/supertest": "^2.0.11",
		"@types/xml2js": "^0.4.9",
		"@typescript-eslint/parser": "^4.33.0",
		"eslint-plugin-import": "^2.20.1",
		"jest": "^27.2.5",
		"pkg": "^5.5.1",
		"run-script-webpack-plugin": "^0.0.11",
		"source-map-support": "^0.5.20",
		"supertest": "^6.1.3",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.6",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.3.5",
		"webpack": "^5.65.0",
		"webpack-cli": "^4.9.1",
		"webpack-node-externals": "^3.0.0"
	},
	"jest": {
		"moduleFileExtensions": [
			"js",
			"json",
			"ts"
		],
		"rootDir": "src",
		"testRegex": ".*\\.spec\\.ts$",
		"transform": {
			"^.+\\.(t|j)s$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.(t|j)s"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node"
	},
	"pkg": {
		"targets": [
			"node16-win-x64"
		]
	}
}
